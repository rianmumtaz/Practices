<% if (typeof error_message !== "undefined" && error_message.length > 0) { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <% error_message.forEach(element => { %>
  <div class="toast align-items-center text-white bg-danger" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-exclamation-triangle-fill"></i> <%= element.msg %>
      </div>
      <button
        type="button"
        class="btn-close btn-close-white me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
  <% }) %>
</div>
<% } %> <% if (typeof success_message !== "undefined" && success_message.length
> 0) { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <% success_message.forEach(element => { %>
  <div class="toast align-items-center text-white bg-success" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-check-circle-fill"></i> <%= element.msg %>
      </div>
      <button
        type="button"
        class="btn-close btn-close-white me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
  <% }) %>
</div>
<% } %> <% if (typeof warning_message !== "undefined" && warning_message.length
> 0) { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <% warning_message.forEach(element => { %>
  <div class="toast align-items-center bg-warning" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-check-circle-fill"></i> <%= element.msg %>
      </div>
      <button
        type="button"
        class="btn-close me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
  <% }) %>
</div>
<% } %> <% if (warning != "") { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <div class="toast align-items-center bg-warning" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-check-circle-fill"></i> <%= warning %>
      </div>
      <button
        type="button"
        class="btn-close me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
</div>
<% } %> <% if (success != "") { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <div class="toast align-items-center bg-success text-white" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-check-circle-fill"></i> <%= success %>
      </div>
      <button
        type="button"
        class="btn-close btn-close-white me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
</div>
<% } %><% if (error != "") { %>
<div class="toast-container position-fixed top-60px end-0 p-3">
  <div class="toast align-items-center text-white bg-danger" role="alert">
    <div class="d-flex">
      <div class="toast-body">
        <i class="bi bi-exclamation-triangle-fill"></i> <%= error %>
      </div>
      <button
        type="button"
        class="btn-close btn-close-white me-2 m-auto"
        data-bs-dismiss="toast"
      ></button>
    </div>
  </div>
</div>
<% } %>
